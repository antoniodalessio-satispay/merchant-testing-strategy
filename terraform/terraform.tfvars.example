# Example Terraform variables file
# Copy this file to terraform.tfvars and adjust values as needed

# For LocalStack (default)
aws_region           = "us-east-1"
aws_access_key       = "test"
aws_secret_key       = "test"
localstack_endpoint  = "http://localhost:4566"
s3_bucket_name       = "merchant-bucket"
dynamodb_table_name  = "merchant-table"
environment          = "test"

# For real AWS (uncomment and set real values)
# aws_region           = "us-east-1"
# aws_access_key       = "YOUR_ACCESS_KEY"
# aws_secret_key       = "YOUR_SECRET_KEY"
# localstack_endpoint  = ""  # Leave empty for real AWS
# s3_bucket_name       = "prod-merchant-bucket"
# dynamodb_table_name  = "prod-merchant-table"
# environment          = "production"
